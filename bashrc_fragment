[ -f ~/.ccgrc ] && source ~/.ccgrc

# PyDev goodies
export PYDEV_DIR="$HOME/src/pydev"

pydev_update() {
    if [ -d "$PYDEV_DIR" ]
    then
        pushd "$PYDEV_DIR"
        source "bin/.bashrc"
        popd
    fi
}

# need to supress pushd/popd output or else running this function
# at login will break ssh/scp.
#pydev_update
